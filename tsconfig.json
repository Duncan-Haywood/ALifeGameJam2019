{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"sourceMap": true,
		"moduleResolution": "node",
		"downlevelIteration": true,
		"removeComments": false,
		"outDir": "out",
		"baseUrl": "./",
		"paths": {
			"p5": [
				"src/@types/p5"
			]
		},
		"typeRoots": [
			"src/@types"
		]
	},
	"include": [
		"src/**/*"
	]
}